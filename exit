[32m+[m[32m        caret: function(begin, end) {[m
[32m+[m[32m            var range;[m
[32m+[m[32m            if (0 !== this.length && !this.is(":hidden")) return "number" == typeof begin ? (end = "number" == typeof end ? end : begin,[m
[32m+[m[32m                this.each(function() {[m
[32m+[m[32m                    this.setSelectionRange ? this.setSelectionRange(begin, end) : this.createTextRange && (range = this.createTextRange(),[m
[32m+[m[32m                        range.collapse(!0), range.moveEnd("character", end), range.moveStart("character", begin),[m
[32m+[m[32m                        range.select());[m
[32m+[m[32m                })) : (this[0].setSelectionRange ? (begin = this[0].selectionStart, end = this[0].selectionEnd) : document.selection && document.selection.createRange && (range = document.selection.createRange(),[m
[32m+[m[32m                begin = 0 - range.duplicate().moveStart("character", -1e5), end = begin + range.text.length),[m
[32m+[m[32m            {[m
[32m+[m[32m                begin: begin,[m
[32m+[m[32m                end: end[m
[32m+[m[32m            });[m
[32m+[m[32m        },[m
[32m+[m[32m        unmask: function() {[m
[32m+[m[32m            return this.trigger("unmask");[m
[32m+[m[32m        },[m
[32m+[m[32m        mask: function(mask, settings) {[m
[32m+[m[32m            var input, defs, tests, partialPosition, firstNonMaskPos, lastRequiredNonMaskPos, len, oldVal;[m
[32m+[m[32m            if (!mask && this.length > 0) {[m
[32m+[m[32m                input = $(this[0]);[m
[32m+[m[32m                var fn = input.data($.mask.dataName);[m
[32m+[m[32m                return fn ? fn() : void 0;[m
[32m+[m[32m            }[m
[32m+[m[32m            return settings = $.extend({[m
[32m+[m[32m                autoclear: $.mask.autoclear,[m
[32m+[m[32m                placeholder: $.mask.placeholder,[m
[32m+[m[32m                completed: null[m
[32m+[m[32m            }, settings), defs = $.mask.definitions, tests = [], partialPosition = len = mask.length,[m
[32m+[m[32m                firstNonMaskPos = null, $.each(mask.split(""), function(i, c) {[m
[32m+[m[32m                "?" == c ? (len--, partialPosition = i) : defs[c] ? (tests.push(new RegExp(defs[c])),[m
[32m+[m[32m                null === firstNonMaskPos && (firstNonMaskPos = tests.length - 1), partialPosition > i && (lastRequiredNonMaskPos = tests.length - 1)) : tests.push(null);[m
[32m+[m[32m            }), this.trigger("unmask").each(function() {[m
[32m+[m[32m                function tryFireCompleted() {[m
[32m+[m[32m                    if (settings.completed) {[m
[32m+[m[32m                        for (var i = firstNonMaskPos; lastRequiredNonMaskPos >= i; i++) if (tests[i] && buffer[i] === getPlaceholder(i)) return;[m
[32m+[m[32m                        settings.completed.call(input);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                function getPlaceholder(i) {[m
[32m+[m[32m                    return settings.placeholder.charAt(i < settings.placeholder.length ? i : 0);[m
[32m+[m[32m                }[m
[32m+[m[32m                function seekNext(pos) {[m
[32m+[m[32m                    for (;++pos < len && !tests[pos]; ) ;[m
[32m+[m[32m                    return pos;[m
[32m+[m[32m                }[m
[32m+[m[32m                function seekPrev(pos) {[m
[32m+[m[32m                    for (;--pos >= 0 && !tests[pos]; ) ;[m
[32m+[m[32m                    return pos;[m
[32m+[m[32m                }[m
[32m+[m[32m                function shiftL(begin, end) {[m
[32m+[m[32m                    var i, j;[m
[32m+[m[32m                    if (!(0 > begin)) {[m
[32m+[m[32m                        for (i = begin, j = seekNext(end); len > i; i++) if (tests[i]) {[m
[32m+[m[32m                            if (!(len > j && tests[i].test(buffer[j]))) break;[m
[32m+[m[32m                            buffer[i] = buffer[j], buffer[j] = getPlaceholder(j), j = seekNext(j);[m
[32m+[m[32m                        }[m
[32m+[m[32m                        writeBuffer(), input.caret(Math.max(firstNonMaskPos, begin));[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                function shiftR(pos) {[m
[32m+[m[32m                    var i, c, j, t;[m
[32m+[m[32m                    for (i = pos, c = getPlaceholder(pos); len > i; i++) if (tests[i]) {[m
[32m+[m[32m                        if (j = seekNext(i), t = buffer[i], buffer[i] = c, !(len > j && tests[j].test(t))) break;[m
[32m+[m[32m                        c = t;[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                function androidInputEvent() {[m
[32m+[m[32m                    var curVal = input.val(), pos = input.caret();[m
[32m+[m[32m                    if (oldVal && oldVal.length && oldVal.length > curVal.length) {[m
[32m+[m[32m                        for (checkVal(!0); pos.begin > 0 && !tests[pos.begin - 1]; ) pos.begin--;[m
[32m+[m[32m                        if (0 === pos.begin) for (;pos.begin < firstNonMaskPos && !tests[pos.begin]; ) pos.begin++;[m
[32m+[m[32m                        input.caret(pos.begin, pos.begin);[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        for (checkVal(!0); pos.begin < len && !tests[pos.begin]; ) pos.begin++;[m
[32m+[m[32m                        input.caret(pos.begin, pos.begin);[m
[32m+[m[32m                    }[m
[32m+[m[32m                    tryFireCompleted();[m
[32m+[m[32m                }[m
[32m+[m[32m                function blurEvent() {[m
[32m+[m[32m                    checkVal(), input.val() != focusText && input.change();[m
[32m+[m[32m                }[m
[32m+[m[32m                function keydownEvent(e) {[m
[32m+[m[32m                    if (!input.prop("readonly")) {[m
[32m+[m[32m                        var pos, begin, end, k = e.which || e.keyCode;[m
[32m+[m[32m                        oldVal = input.val(), 8 === k || 46 === k || iPhone && 127 === k ? (pos = input.caret(),[m
[32m+[m[32m                            begin = pos.begin, end = pos.end, end - begin === 0 && (begin = 46 !== k ? seekPrev(begin) : end = seekNext(begin - 1),[m
[32m+[m[32m                            end = 46 === k ? seekNext(end) : end), clearBuffer(begin, end), shiftL(begin, end - 1),[m
[32m+[m[32m                            e.preventDefault()) : 13 === k ? blurEvent.call(this, e) : 27 === k && (input.val(focusText),[m
[32m+[m[32m                            input.caret(0, checkVal()), e.preventDefault());[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                function keypressEvent(e) {[m
[32m+[m[32m                    if (!input.prop("readonly")) {[m
[32m+[m[32m                        var p, c, next, k = e.which || e.keyCode, pos = input.caret();[m
[32m+[m[32m                        if (!(e.ctrlKey || e.altKey || e.metaKey || 32 > k) && k && 13 !== k) {[m
[32m+[m[32m                            if (pos.end - pos.begin !== 0 && (clearBuffer(pos.begin, pos.end), shiftL(pos.begin, pos.end - 1)),[m
[32m+[m[32m                                    p = seekNext(pos.begin - 1), len > p && (c = String.fromCharCode(k), tests[p].test(c))) {[m
[32m+[m[32m                                if (shiftR(p), buffer[p] = c, writeBuffer(), next = seekNext(p), android) {[m
[32m+[m[32m                                    var proxy = function() {[m
[32m+[m[32m                                        $.proxy($.fn.caret, input, next)();[m
[32m+[m[32m                                    };[m
[32m+[m[32m                                    setTimeout(proxy, 0);[m
[32m+[m[32m                                } else input.caret(next);[m
[32m+[m[32m                                pos.begin <= lastRequiredNonMaskPos && tryFireCompleted();[m
[32m+[m[32m                            }[m
[32m+[m[32m                            e.preventDefault();[m
[32m+[m[32m                        }[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m[32m                function clearBuffer(start, end) {[m
[32m+[m[32m                    var i;[m
[32m+[m[32m                    for (i = start; end > i && len > i; i++) tests[i] && (buffer[i] = getPlaceholder(i));[m
[32m+[m[32m                }[m
[32m+[m[32m                function writeBuffer() {[m
[32m+[m[32m                    input.val(buffer.join(""));[m
[32m+[m[32m                }[m
[32m+[m[32m                function checkVal(allow) {[m
[32m+[m[32m                    var i, c, pos, test = input.val(), lastMatch = -1;[m
[32m+[m[32m                    for (i = 0, pos = 0; len > i; i++) if (tests[i]) {[m
[32m+[m[32m                        for (buffer[i] = getPlaceholder(i); pos++ < test.length; ) if (c = test.charAt(pos - 1),[m
[32m+[m[32m                                tests[i].test(c)) {[m
[32m+[m[32m                            buffer[i] = c, lastMatch = i;[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        }[m
[32m+[m[32m                        if (pos > test.length) {[m
[32m+[m[32m                            clearBuffer(i + 1, len);[m
[32m+[m[32m                            break;[m
[32m+[m[32m                        }[m
[32m+[m[32m                    } else buffer[i] === test.charAt(pos) && pos++, partialPosition > i && (lastMatch = i);[m
[32m+[m[32m                    return allow ? writeBuffer() : partialPosition > lastMatch + 1 ? settings.autoclear || buffer.join("") === defaultBuffer ? (input.val() && input.val(""),[m
[32m+[m[32m                        clearBuffer(0, len)) : writeBuffer() : (writeBuffer(), input.val(input.val().substring(0, lastMatch + 1))),[m
[32m+[m[32m                        partialPosition ? i : firstNonMaskPos;[m
[32m+[m[32m                }[m
[32m+[m[32m                var input = $(this), buffer = $.map(mask.split(""), function(c, i) {[m
[32m+[m[32m                    return "?" != c ? defs[c] ? getPlaceholder(i) : c : void 0;[m
[32m+[m[32m                }), defaultBuffer = buffer.join(""), focusText = input.val();[m
[32m+[m[32m                input.data($.mask.dataName, function() {[m
[32m+[m[32m                    return $.map(buffer, function(c, i) {[m
[32m+[m[32m                        return tests[i] && c != getPlaceholder(i) ? c : null;[m
[32m+[m[32m                    }).join("");[m
[32m+[m[32m                }), input.one("unmask", function() {[m
[32m+[m[32m                    input.off(".mask").removeData($.mask.dataName);[m
[32m+[m[32m                }).on("focus.mask", function() {[m
[32m+[m[32m                    if (!input.prop("readonly")) {[m
[32m+[m[32m                        clearTimeout(caretTimeoutId);[m
[32m+[m[32m                        var pos;[m
[32m+[m[32m                        focusText = input.val(), pos = checkVal(), caretTimeoutId = setTimeout(function() {[m
[32m+[m[32m                            input.get(0) === document.activeElement && (writeBuffer(), pos == mask.replace("?", "").length ? input.caret(0, pos) : input.caret(pos));[m
[32m+[m[32m                        }, 10);[m
[32m+[m[32m                    }[m
[32m+[m[32m                }).on("blur.mask", blurEvent).on("keydown.mask", keydownEvent).on("keypress.mask", keypressEvent).on("input.mask paste.mask", function() {[m
[32m+[m[32m                    input.prop("readonly") || setTimeout(function() {[m
[32m+[m[32m                        var pos = checkVal(!0);[m
[32m+[m[32m                        input.caret(pos), tryFireCompleted();[m
[32m+[m[32m                    }, 0);[m
[32m+[m[32m                }), chrome && android && input.off("input.mask").on("input.mask", androidInputEvent),[m
[32m+[m[32m                    checkVal();[m
[32m+[m[32m            });[m
[32m+[m[32m        }[m
[32m+[m[32m    });[m
[32m+[m[32m});[m
