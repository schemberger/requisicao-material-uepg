[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            else {[m
[32m+[m[32m                document.getElementById("button1id").disabled = false;[m
[32m+[m[32m                $("#showmessageANO1").hide();  //O Ano do Relatório CGE é obrigatório.[m
[32m+[m
[32m+[m[32m                $("#showmessageNR").hide();  //O Número do Relatório CGE é obrigatório.[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
