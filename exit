[32m+[m[32m$(document).ready(function () {[m
[32m+[m[32m    $("#nrRelatorioCGE").on('change', function (e) {[m
[32m+[m
[32m+[m[32m        var nr = document.getElementById('nrRelatorioCGE').value;[m
[32m+[m
[32m+[m[32m        var ano = document.getElementById('nrAnoRelatorioCGE').value;[m
[32m+[m
[32m+[m[32m        valida(nr, ano);[m
[32m+[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    $("#nrAnoRelatorioCGE").on('change', function (e) {[m
[32m+[m
[32m+[m[32m        var nr = document.getElementById('nrRelatorioCGE').value;[m
[32m+[m
[32m+[m[32m        var ano = document.getElementById('nrAnoRelatorioCGE').value;[m
[32m+[m
[32m+[m[32m        valida(nr, ano);[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    function valida(nr, ano) {[m
[32m+[m
[32m+[m[32m        var anoAtual = $.datepicker.formatDate('yy', new Date());[m
[32m+[m
[32m+[m[32m        if (nr) {[m
[32m+[m[32m            $("#showmessageNR").hide();  //O Número do Relatório CGE é obrigatório.[m
[32m+[m[32m            if(nr > 0){[m
[32m+[m
[32m+[m[32m                $("#showmessageNR1").hide();  //Número do Relatório CGE inválido.[m
[32m+[m
[32m+[m[32m                if (ano) {[m
[32m+[m
[32m+[m[32m//                            $("#showmessageNR").hide();  //O Número do Relatório CGE é obrigatório.[m
[32m+[m[32m                    $("#showmessageANO1").hide();  //O Ano do Relatório CGE é obrigatório.[m
[32m+[m
[32m+[m[32m                    if (ano <= anoAtual && ano >= 2010) {[m
[32m+[m
[32m+[m[32m                        $("#showmessageANO").hide();  //Ano do Relatório inválido.[m
[32m+[m
[32m+[m[32m                        document.getElementById("button1id").disabled = false;[m
[32m+[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        document.getElementById("button1id").disabled = true;[m
[32m+[m[32m                        $("#showmessageANO").show();  //Ano do Relatório inválido.[m
[32m+[m[32m                    }[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    document.getElementById("button1id").disabled = true;[m
[32m+[m[32m                    $("#showmessageANO1").show();  //Ano do Relatório CGE é obrigatório.[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m            }else{[m
[32m+[m[32m                if(ano){[m
[32m+[m[32m                    if (ano <= anoAtual && ano >= 2010) {[m
[32m+[m
[32m+[m[32m                        $("#showmessageANO").hide();  //Ano do Relatório inválido.[m
[32m+[m
[32m+[m[32m                        document.getElementById("button1id").disabled = false;[m
[32m+[m
[32m+[m[32m                    } else {[m
[32m+[m[32m                        document.getElementById("button1id").disabled = true;[m
[32m+[m[32m                        $("#showmessageANO").show();  //Ano do Relatório inválido.[m
[32m+[m[32m                    }[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                $("#showmessageNR1").show();  //Número do Relatório CGE inválido.[m
[32m+[m[32m                document.getElementById("button1id").disabled = true;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        } else {[m
[32m+[m
[32m+[m[32m            if (ano) {[m
[32m+[m
[32m+[m[32m                document.getElementById("button1id").disabled = true;[m
[32m+[m[32m                $("#showmessageNR").show();  //O Número do Relatório CGE é obrigatório.[m
[32m+[m
[32m+[m[32m                if (ano > anoAtual || ano < 2010) {[m
[32m+[m
[32m+[m[32m                    document.getElementById("button1id").disabled = true;[m
[32m+[m[32m                    $("#showmessageANO").show();  //Ano do Relatório CGE é obrigatório.[m
[32m+[m
[32m+[m[32m                } else {[m
