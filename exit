[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mabstract class TestCase extends Illuminate\Foundation\Testing\TestCase[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The base URL to use while testing the application.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @var string[m
[32m+[m[32m     */[m
[32m+[m[32m    protected $baseUrl = 'http://localhost';[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Creates the application.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return \Illuminate\Foundation\Application[m
[32m+[m[32m     */[m
[32m+[m[32m    public function createApplication()[m
[32m+[m[32m    {[m
[32m+[m[32m        $app = require __DIR__.'/../bootstrap/app.php';[m
[32m+[m
[32m+[m[32m        $app->make(Illuminate\Contracts\Console\Kernel::class)->bootstrap();[m
[32m+[m
[32m+[m[32m        return $app;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
