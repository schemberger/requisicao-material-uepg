[1mdiff --git a/app/Http/Controllers/MaterialController.php b/app/Http/Controllers/MaterialController.php[m
[1mnew file mode 100644[m
[1mindex 0000000..b0d1412[m
[1m--- /dev/null[m
[1m+++ b/app/Http/Controllers/MaterialController.php[m
[36m@@ -0,0 +1,123 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32muse Illuminate\Http\RedirectResponse;[m
[32m+[m[32muse Illuminate\Http\Request;[m
[32m+[m[32muse Illuminate\Support\Facades\DB;[m
[32m+[m[32muse Illuminate\Support\Facades\Auth;[m
[32m+[m
[32m+[m[32mclass MaterialController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Display a listing of the resource.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return \Illuminate\Http\Response[m
[32m+[m[32m     */[m
[32m+[m[32m    public function index()[m
[32m+[m[32m    {[m
[32m+[m
[32m+[m[32m        // para pegar o valor do ano acessar o banco e recuperar o ultimo ano da requisicao e fazer um ++ ate o ano atual[m
[32m+[m
[32m+[m[32m        $ano_rm = DB::table('Requisicao_Material')[m
[32m+[m[32m            ->min('ano_rm');[m
[32m+[m
[32m+[m[32m        $ano_atual = date('Y');[m
[32m+[m
[32m+[m[32m        $usuario = Auth::user()->username;[m
[32m+[m
[32m+[m[32m        $acesso = DB::select('EXEC acesso_g..ver_acessos_nivel ?, ?, ?, ?, ?', array('Compras', '', '', 'trrebonato', 0));[m
[32m+[m
[32m+[m[32m        //select abaixo sera utilizado quando subir pra prod, enquanto estiver em testes usar usuario trrebonato[m
[32m+[m[32m//        $acesso = DB::select('EXEC acesso_g..ver_acessos_nivel ?, ?, ?, ?, ?', array('Compras', '', '', $usuario, 0));[m
[32m+[m
[32m+[m[32m        if (count($acesso)) {[m
[32m+[m
[32m+[m[32m            $nm_centro =array();[m
[32m+[m
[32m+[m[32m            foreach ($acesso as $i => $aux) {[m
[32m+[m
[32m+[m[32m                $nm_centro[] = DB::table('asseplan..centro_custo')[m
[32m+[m[32m                        ->where('cd_centro', $aux->nivel_acesso)[m
[32m+[m[32m                        ->first();[m
[32m+[m[32m            }[m
[32m+[m[32m        } else {[m
[32m+[m[32m            $nm_centro = DD::table('asseplan..centro_custo')[m
[32m+[m[32m                ->select('nm_centro')[m
[32m+[m[32m                ->where('tp_ativ_dest', 'N')[m
[32m+[m[32m                ->get();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return view('material.index', compact('ano_rm', 'ano_atual', 'nm_centro'));[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function showTable(Request $request){[m
[32m+[m
[32m+[m[32m    }[m
