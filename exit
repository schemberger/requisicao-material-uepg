[32m+[m[32m/**[m
[32m+[m[32m * Created by schem on 02/02/2017.[m
[32m+[m[32m */[m
[32m+[m[32mfunction limpa_formulário_cpf() {[m
[32m+[m[32m// Limpa valores do formulário de cpf.[m
[32m+[m[32m    $("#nomeResp").val("");[m
[32m+[m[32m    $("#cep").val("");[m
[32m+[m[32m    $("#enderecoResp").val("");[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m//Quando o campo cpf perde o foco.[m
[32m+[m[32m$("#cpf").blur(function () {[m
[32m+[m
[32m+[m[32m    //Nova variável "cpf" somente com dígitos.[m
[32m+[m[32m    var cpf = $(this).val().replace(/\D/g, '');[m
[32m+[m
[32m+[m[32m    if (cpf != "") {[m
[32m+[m
[32m+[m[32m        //imagem loagin.gif enquanto faz a requisicao ajax[m
[32m+[m[32m        $("#loadingmessage").show();[m
[32m+[m
[32m+[m[32m        //Preenche os campos com "..." enquanto consulta webservice.[m
[32m+[m[32m        $("#nomeResp").val("...");[m
[32m+[m[32m        $("#cep").val("...");[m
[32m+[m[32m        $("#enderecoResp").val("...");[m
[32m+[m
[32m+[m[32m        var get_nameURL = $(this).attr('data-url')+'/'+cpf;[m
[32m+[m
[32m+[m[32m        $.get(get_nameURL, function (data) {[m
[32m+[m
[32m+[m[32m            if (data[0].computed0 != null) {[m
[32m+[m
[32m+[m[32m                //Atualiza os campos com os valores da consulta.[m
[32m+[m[32m                $("#nomeResp").val(data[0].computed0);[m
[32m+[m[32m                $("#cep").val(data[0].computed2);[m
[32m+[m[32m                $("#enderecoResp").val(data[0].computed1);[m
[32m+[m
[32m+[m[32m                $('#loadingmessage').hide(); // hide the loading message[m
[32m+[m[32m            }else{[m
[32m+[m[32m                alert("Cpf não foi encontrado.");[m
[32m+[m[32m                $('#loadingmessage').hide(); // hide the loading message[m
[32m+[m[32m                limpa_formulário_cpf();[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
[32m+[m
[32m+[m
