[32m+[m[32mif (typeof jQuery === 'undefined') {[m
[32m+[m[32m  throw new Error('Bootstrap\'s JavaScript requires jQuery')[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m+function ($) {[m
[32m+[m[32m  'use strict';[m
[32m+[m[32m  var version = $.fn.jquery.split(' ')[0].split('.')[m
[32m+[m[32m  if ((version[0] < 2 && version[1] < 9) || (version[0] == 1 && version[1] == 9 && version[2] < 1) || (version[0] > 3)) {[m
[32m+[m[32m    throw new Error('Bootstrap\'s JavaScript requires jQuery version 1.9.1 or higher, but lower than version 4')[m
[32m+[m[32m  }[m
[32m+[m[32m}(jQuery);[m
