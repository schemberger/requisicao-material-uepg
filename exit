[32m+[m[32m/**[m
[32m+[m[32m * Created by schem on 02/02/2017.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m//Quando o campo cpf perde o foco.[m
[32m+[m[32m$("#cep").blur(function () {[m
[32m+[m
[32m+[m[32m    //Nova variável "cep" somente com dígitos.[m
[32m+[m[32m    var cep = $(this).val().replace(/\D/g, '');[m
[32m+[m
[32m+[m[32m    if (cep != "") {[m
[32m+[m
[32m+[m[32m        $("#enderecoResp").val("...");[m
[32m+[m
[32m+[m[32m        var get_nameURL = $(this).attr('data-url') + '/' + cep;[m
[32m+[m
[32m+[m[32m        $.get(get_nameURL, function (data) {[m
[32m+[m
[32m+[m[32m            if (!("erro" in data) || data != "") {[m
[32m+[m[32m                console.log(data[0]);[m
[32m+[m
[32m+[m[32m                //Atualiza os campos com os valores da consulta.[m
[32m+[m[32m                $("#enderecoResp").val(data[0].NM_TIPO_LOGRADOURO + ' ' + data[0].DS_ENDERECO + ', ' + data[0].NM_BAIRRO + ' '[m
[32m+[m[32m                    + data[0].NM_CIDADE + ' ' + data[0].UF_SIGLA);[m
[32m+[m
[32m+[m[32m            } else {[m
[32m+[m[32m                alert("CEP não foi encontrado.");[m
[32m+[m[32m            }[m
[32m+[m[32m        });[m
[32m+[m[32m    }[m
[32m+[m[32m});[m
