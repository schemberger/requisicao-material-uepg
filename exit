[32m+[m[32m    var caretTimeoutId, ua = navigator.userAgent, iPhone = /iphone/i.test(ua), chrome = /chrome/i.test(ua), android = /android/i.test(ua);[m
[32m+[m[32m    $.mask = {[m
[32m+[m[32m        definitions: {[m
[32m+[m[32m            "9": "[0-9]",[m
