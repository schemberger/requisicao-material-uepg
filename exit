[36m@@ -0,0 +1,45 @@[m
[32m+[m
[32m+[m[32mwindow._ = require('lodash');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * We'll load jQuery and the Bootstrap jQuery plugin which provides support[m
[32m+[m[32m * for JavaScript based Bootstrap features such as modals and tabs. This[m
[32m+[m[32m * code may be modified to fit the specific needs of your application.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mwindow.$ = window.jQuery = require('jquery');[m
[32m+[m[32mrequire('bootstrap-sass');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Vue is a modern JavaScript library for building interactive web interfaces[m
[32m+[m[32m * using reactive data binding and reusable components. Vue's API is clean[m
[32m+[m[32m * and simple, leaving you to focus on building your next great project.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mwindow.Vue = require('vue');[m
[32m+[m[32mrequire('vue-resource');[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * We'll register a HTTP interceptor to attach the "CSRF" header to each of[m
[32m+[m[32m * the outgoing requests issued by this application. The CSRF middleware[m
[32m+[m[32m * included with Laravel will automatically verify the header's value.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32mVue.http.interceptors.push((request, next) => {[m
[32m+[m[32m    request.headers.set('X-CSRF-TOKEN', Laravel.csrfToken);[m
[32m+[m
[32m+[m[32m    next();[m
[32m+[m[32m});[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Echo exposes an expressive API for subscribing to channels and listening[m
[32m+[m[32m * for events that are broadcast by Laravel. Echo and event broadcasting[m
[32m+[m[32m * allows your team to easily build robust real-time web applications.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32m// import Echo from "laravel-echo"[m
[32m+[m
[32m+[m[32m// window.Echo = new Echo({[m
[32m+[m[32m//     broadcaster: 'pusher',[m
[32m+[m[32m//     key: 'your-pusher-key'[m
[32m+[m[32m// });[m
